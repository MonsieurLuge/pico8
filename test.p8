pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--level = {x=0,y=0,width=10,height=10}

function _init()
   level = levels:start(1)
end

function _update()
  if btnp(0) then
    move(
      level,
      level.player.position,
      {x=-1,y=0}
    )
  end
  if btnp(1) then
    move(
      level,
      level.player.position,
      {x=1,y=0}
    )
  end
  if btnp(2) then
    move(
      level,
      level.player.position,
      {x=0,y=-1}
    )
  end
  if btnp(3) then
    move(
      level,
      level.player.position,
      {x=0,y=1}
    )
  end
end

function _draw()
  cls()
  draw_level(level)
  draw_player(level.player.position)
end

-- levels ---------------------

levels = {}

function levels:start(lvl)
  local level = {}

  if lvl == 1 then
    level.x = 0
    level.y = 0
    level.width = 7
    level.height = 6  
  end

  self:content(level)

  return level
end

function levels:content(level)
  level.content = {}
  for x=0,level.width -1 do
    level.content[x] = {}
    for y=0,level.height -1 do
      local tile = mget(x,y)
      if tile == 1 then
        level.player = {
          position = {x=x,y=y}
        }
      end
      if tile == 4 then
        level.content[x][y] = {
          sprite = 4,
          moveable = false,
          traversable = true
        }
      elseif tile == 5 then
        level.content[x][y] = {
          sprite = 5,
          moveable = false,
          traversable = false
        }
      else
        level.content[x][y] = {
          sprite = 0,
          moveable = false,
          traversable = true
        }
      end
    end
  end
end

function draw_level(level)
  for y=0,level.height - 1 do
    for x=0,level.width - 1 do
      local sprite = level.content[x][y]
      if sprite.sprite > 0 then
        spr(sprite.sprite,x * 8,y * 8)
      end
    end
  end
end

function draw_player(pos)
  spr(1,pos.x * 8,pos.y * 8)
end

function move(level,pos,delta)
  if can_move(level,pos,delta) then
    level.player.position.x += delta.x
    level.player.position.y += delta.y
  end
end

function can_move(level,pos,delta)
  return true
end
