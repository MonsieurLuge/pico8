background=function()
  cls()
  for y=0,15 do
    for x=0,15 do
      spr(6,x*8,y*8)
    end
  end
end

build=function(def)
  local walls={}
  local objects={}
  local action=objects.add
  for y=def.y,def.y+def.height do
    for x=def.x,def.x+def.width do
      object(mget(x,y), action)
    end
  end
  return stage:new(walls,objects)
end

