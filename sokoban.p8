--update functions
function updateplayer()
 dx=0
 dy=0

 if (btn(⬅️)) dx=-1
 if (btn(➡️)) dx=1
 if (btn(⬆️)) dy=-1
 if (btn(⬇️)) dy=1

	move(player,dx,dy)
end
