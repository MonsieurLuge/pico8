pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

function _draw()
  background()
  current:draw()
end

function _init()
  game=sokoban:new()
  game:start()
end

function _update()
  -- todo
end

background=function()
  cls()
  for y=0,15 do
    for x=0,15 do
      spr(33,x*8,y*8)
    end
  end
end

box={
  new=function(self,sprite)
    return {
      sprite=sprite
    }
  end
}

build=function(def)
  return stage:new(
    def.width,
    def.height,
    groundelements(def),
    objectsfromdef(def)
  )
end

ground={
  new=function(self)
    return {
      sprite=19,
      traversable=true
    }
  end
}

groundelement=function(id)
  if isin(id,{16,17,18,20,32,33,34,35,36,37,48,49,50,52}) then
    return wall:new(id)
  elseif isin(id,{3, 4}) then
    return switch:new(id)
  else
    return ground:new()
  end
end

groundelements=function(def)
  local elements={}
  for x=1,def.width do
    elements[x]={}
  end
  for y=1,def.height do
    for x=1,def.width do
      elements[x][y]=groundelement(
        mget(x-1,y-1)
      )
    end
  end
  return elements
end

isin=function(item,list)
  for i in all(list) do
    if (i==item) return true
  end
  return false
end

object=function(id,action)
  objects:find(id,action,objectnotfound)
end

objectnotfound=function(id)
  print("object ["..id.."] not found")
end

objects={
  find=function(self,id,onsuccess,onerror)
    if self[id] then
      onsuccess(self[id])
    else
      onerror(id)
    end
  end,
  -- {id=1},
  {id=2},
  {id=3},
  {id=4},
  {id=5}
}

objectsfromdef=function(def)
  local objects={}
  for y=1,def.height do
    for x=1,def.width do
      local id=mget(x-1,y-1)
      if id==1 then
        -- player
        add(objects,{sprite=1,x=x,y=y})
      elseif id==2 then
        -- box
        add(objects,{sprite=2,x=x,y=y})
      elseif id==3 then
        -- placed box
        add(objects,{sprite=2,x=x,y=y})
      end
    end
  end
  return objects
end

player={
  new=function(self)
    return {
      sprit=1
    }
  end
}

sokoban={
  new=function(self)
    return {
      nb=1,
      start=function(self)
        current=build(stagesdef(self.nb))
        current:start()
      end,
      stop=function(self)
        -- todo
      end
    }
  end
}

stage={
  new=function(self,width,height,walls,objects)
    return {
      height=height,
      objects=objects,
      walls=walls,
      width=width,
      draw=function(self)
        -- background
        rectfill(0,0,self.width*8-1,self.height*8-1,0)
        -- ground elements
        for y=1,self.height do
          for x=1,self.width do
            spr(walls[x][y].sprite,x*8-8,y*8-8)
          end
        end
        -- objects
        for obj in all(self.objects) do
          spr(
            obj.sprite,
            obj.x*8-8,
            obj.y*8-8
          )
        end
      end,
      start=function(self)
        -- todo
      end
    }
  end
}

stagesdef=function(nb)
  local stagesdef={
    {x=0,y=0,width=9,height=7}
  }
  return stagesdef[nb]
end

switch={
  new=function(self)
    return {
      sprite=4,
      traversable=true
    }
  end
}

wall={
  new=function(self,sprite)
    return {
      sprite=sprite,
      traversable=false
    }
  end
}

