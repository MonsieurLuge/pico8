version 16
__lua__
function _init()
 newstage(1)
 newui()
end

function _draw()
 drawstage()
 drawui()
end

function _update()
 updateplayer()
end

function loadelements(x,y,w,h)
 local elts={}

 for i=1,w do
  for j=1,h do
   newelement(elts,mget(x+i-1,y+j-1),i,j)
  end
 end

 return elts
end

function loadground(x,y,w,h)
 local grd={}

 for i=1,w do
  grd[i]={}

  for j=1,h do
   local id=mget(x+i-1,y+j-1)
   grd[i][j]=newstatic(id,i,j)
  end
 end

 return grd
end

function onswitch(x,y)
 local nx=x/8+1
 local ny=y/8+1
 local grd=stage.ground[nx][ny]
 if (grd==nil) return false
 return fget(grd.id,1)
end

function move(obj,dx,dy)
 local nx=obj.x+dx
 local ny=obj.y+dy
 local next=stage:atpos(nx,ny)

 if (collision(obj,next)) return

 obj.x=nx
 obj.y=ny
end

function collision(a,b)
 if a.hitbox and b.hitbox then
  return a.hitbox.collide(b.hitbox)
 end

 return false
end
-->8
--factories

function newstage(lvl)
 local stages={
  {x=0,y=0,w=9,h=7,name="the start"}
 }

 stage=stages[lvl]

 stage.ground=loadground(stage.x,stage.y,stage.w,stage.h)
 stage.elements=loadelements(stage.x,stage.y,stage.w,stage.h)
 stage.empty={}

 stage.atpos=function(self,x,y)
  local nx=flr(x/8+1)
  local ny=flr(y/8+1)

  if (nx<1 or nx>stage.w or ny<1 or ny>stage.h) return newwall(33,x,y)

  if (stage.ground[nx][ny]) return stage.ground[nx][ny]
  return stage.empty
 end
end

function newstatic(id,x,y)
 local nx=x*8-8
 local ny=y*8-8
 --wall
 if (fget(id,0)) return newwall(id,nx,ny)
 --switch
 if (fget(id,1)) return newswitch(nx,ny)
end

function newelement(elts,id,x,y)
 local nx=x*8-8
 local ny=y*8-8
 if (fget(id,2)) add(elts,newbox(nx,ny))
 if (id==1) player=newplayer(nx,ny)
end

function newplayer(x,y)
 local p={}

 p.x=x
 p.y=y
 p.hitbox=newhitbox(p,1,1,5,5)
 p.anim=function()
  return 1
 end
 p.draw=function()
  p.hitbox.draw(10)
  drawsprite(p,p.x,p.y)
 end

 return p
end

function newhitbox(obj,dx,dy,w,h)
 local hb={}

 hb.dx=dx
 hb.dy=dy
 hb.w=w
 hb.h=h
 hb.obj=obj
 hb.collide=function(hb2)
  local l1=hb.obj.x+hb.dx --left
  local t1=hb.obj.y+hb.dy --top
  local l2=hb2.obj.x+hb2.dx --left
  local t2=hb2.obj.y+hb2.dy --top
  
  return not (
   l1>l2+hb2.w
   or l1+hb.w<l2
   or t1>t2+hb2.h
   or t1+hb.h<t2
  )
 end
 hb.draw=function(c)
  local nx=hb.obj.x+hb.dx
  local ny=hb.obj.y+hb.dy
  local wx=nx+hb.w
  local hy=ny+hb.h

  line(nx,ny,wx,ny,c)
  line(wx,ny,wx,hy,c)
  line(wx,hy,nx,hy,c)
  line(nx,hy,nx,ny,c)
 end

 return hb
end

function newbox(x,y)
 local box={}

 box.x=x
 box.y=y
 box.anim=function()
  if (onswitch(x,y)) return 3
  return 2
 end
 box.draw=function()
  drawsprite(box,x,y)
 end

 return box
end

function newwall(id,x,y)
 local wall={}

 wall.id=id
 wall.x=x
 wall.y=y
 wall.hitbox=newhitbox(wall,0,0,7,7)
 wall.anim=function()
  return wall.id
 end
 wall.draw=function()
  drawsprite(wall,x,y)
  wall.hitbox.draw(14)
 end

 return wall
end

function newswitch()
local s={}

 s.id=4
 s.anim=function()
  return s.id
 end
 s.draw=function(x,y)
  drawsprite(s,x,y)
 end

 return s
end

function newui()
 return {
  draw=drawui
 }
end

