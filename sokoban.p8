pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

function _draw()
  background()
  game:draw()
  glitch(90)
end

function _init()
  game=sokoban:new():start()
end

function _update()
  -- todo
end

background=function()
  for y=0,15 do
    for x=0,15 do
      spr(33,x*8,y*8)
    end
  end
end

box={
  new=function(self,x,y,placed)
    return {
      placed=placed,
      x=x,
      y=y,
      draw=function(self,x,y)
        if self.placed then
          spr(3,x,y)
        else
          spr(2,x,y)
        end
      end
    }
  end
}

build=function(def)
  return stage:new(
    def.width,
    def.height,
    groundelements(def),
    objectsfromdef(def)
  )
end

glitch=function(threshold)
  if (flr(rnd(100))<threshold) return
  for n=1,flr(rnd(3)) do
    local xpow=flr(rnd(4))
    local ypow=flr(rnd(3))
    local y=flr(rnd(128))
    for n=128,0,-1 do
      for ny=1,ypow do
        pset(n,y+ny,pget(n-xpow,y+ny))
      end
    end
  end
end

ground={
  new=function(self)
    return {
      traversable=true,
      draw=function(self,x,y)
        spr(52,x,y)
      end
    }
  end
}

groundelement=function(id)
  if isin(id,{16,17,18,19,20,21,32,33,34,35,36,37,48,49,50,51}) then
    return wall:new(id)
  elseif isin(id,{3, 4}) then
    return switch:new(id)
  else
    return ground:new()
  end
end

groundelements=function(def)
  local elements={}
  for x=1,def.width do
    elements[x]={}
  end
  for y=1,def.height do
    for x=1,def.width do
      elements[x][y]=groundelement(
        mget(x-1,y-1)
      )
    end
  end
  return elements
end

isin=function(item,list)
  for i in all(list) do
    if (i==item) return true
  end
  return false
end

objectsfromdef=function(def)
  local objects={}
  for y=1,def.height do
    for x=1,def.width do
      local id=mget(x-1,y-1)
      if id==1 then
        add(objects,player:new(x,y))
      elseif id==2 or id==3 then
        add(objects,box:new(x,y,id==3))
      end
    end
  end
  return objects
end

player={
  new=function(self,x,y)
    return {
      x=x,
      y=y,
      draw=function(self,x,y)
        spr(1,x,y)
      end
    }
  end
}

sokoban={
  new=function(self)
    return {
      nb=1,
      draw=function(self)
        self.stage:draw()
      end,
      pause=function(self)
        return self
        -- todo
      end,
      start=function(self)
        self.stage=build(stagesdef(self.nb))
        self.stage:start()
        return self
      end,
      stop=function(self)
        return self
        -- todo
      end,
      unpause=function(self)
        return self
        -- todo
      end
    }
  end
}

stage={
  new=function(self,width,height,groundel,objects)
    return {
      height=height,
      objects=objects,
      groundel=groundel,
      width=width,
      draw=function(self)
        -- ground elements
        for y=1,self.height do
          for x=1,self.width do
            rectfill(x*8-8,y*8-8,x*8-1,y*8-1,1)
            groundel[x][y]:draw(x*8-8,y*8-8)
          end
        end
        -- objects
        for obj in all(self.objects) do
          obj:draw(obj.x*8-8,obj.y*8-8)
        end
      end,
      start=function(self)
        -- todo
      end
    }
  end
}

stagesdef=function(nb)
  local stagesdef={
    {x=0,y=0,width=9,height=7}
  }
  return stagesdef[nb]
end

switch={
  new=function(self)
    return {
      traversable=true,
      draw=function(self,x,y)
        spr(4,x,y)
      end
    }
  end
}

wall={
  new=function(self,sprite)
    return {
      traversable=false,
      draw=function(self,x,y)
        spr(sprite,x,y)
      end
    }
  end
}

