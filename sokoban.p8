pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
function _init()
 newstage(1)
 newui()
end

function _draw()
 drawstage()
 drawui()
end

function _update()
 updateplayer()
end

function loadelements(x,y,w,h)
 local elts={}
 
 for i=1,w do
  for j=1,h do
   newelement(elts,mget(x+i-1,y+j-1),i,j)
  end 
 end
 
 return elts
end

function loadground(x,y,w,h)
 local grd={}
 
 for i=1,w do
  grd[i]={}
  
  for j=1,h do
   local id=mget(x+i-1,y+j-1)
   grd[i][j]=newgrdelement(id)
  end 
 end
 
 return grd
end

function onswitch(x,y)
 local grd=stage.ground[x][y] 
 if (grd==nil) return false
 return fget(grd.id,1) 
end

function coords(i,j)
 return {
  x=i*8-8,
  y=j*8-8
 }
end

function canmove(pos,move)
 local dx=pos.x+move.x
 local dy=pos.y+move.y
 local next=stage:atpos(dx,dy)

	if (next==nil) return true
 
 if (fget(next.id,2)) return false
 
 return not fget(next.id,0)
end
-->8
--factories

function newstage(lvl)
 local stages={
  {x=0,y=0,w=9,h=7,name="the start"}
 }

 stage=stages[lvl]
 
 stage.ground=loadground(stage.x,stage.y,stage.w,stage.h)
 stage.elements=loadelements(stage.x,stage.y,stage.w,stage.h)
 
 stage.atpos=function(self,x,y)
  if self.elements[x][y]==nil then
   return self.ground[x][y]
  end
  return self.elements[x][y]
 end
end

function newgrdelement(id)
 if (fget(id,0)) return newwall(id)
 if (fget(id,1)) return newswitch()
end

function newelement(elts,id,x,y)
 if (fget(id,2)) add(elts,newbox(x,y))
 if (id==1) player=newplayer(x,y)
end

function newplayer(x,y)
 local p={}
 
 p.x=x
 p.y=y
 p.draw=function()
  drawstatic(1,coords(p.x,p.y))
 end
 
 return p
end

function newbox(x,y)
 return {
  x=x,
  y=y,
  draw=function()
   if onswitch(x,y) then
    drawstatic(3,coords(x,y))
   else
    drawstatic(2,coords(x,y))
   end
  end
 }
end

function newwall(id)
 local wall={}

 wall.id=id
 wall.draw=function(x,y)
  drawstatic(wall.id,coords(x,y))
 end
 
 return wall
end

function newswitch()
local s={}
 
 s.id=4
 s.draw=function(x,y)
  drawstatic(s.id,coords(x,y))
 end

 return s
end

function newui()
 return {
  draw=drawui  
 }
end

-->8
--draw functions

function drawstage()
 local x,y=stage.x,stage.y
 local w,h=stage.w,stage.h
 local xx,yy=x+w,y+h

 camera(flr((w*8-124)/2),flr((h*8-124)/2))
 
 cls(6)
 rectfill(x*8,y*8,xx*8-1,yy*8-1,5) 
 
 for i=1,w do
  for j=1,h do
   local g=stage.ground[i][j]  
   if (g) g.draw(i,j)
  end
 end
 
 for elt in all(stage.elements) do
  elt.draw()
 end
 
 drawplayer()
 
 camera()
 if (debug) print(debug,2,2,2)
end

function drawplayer()
 player.draw()
end

function drawstatic(id,c)
 spr(id,c.x,c.y)
end

function drawground(stage)
 for i=1,stage.w do
  for j=1,stage.h do
   stage.ground[i][j]:draw(i,j)
  end
 end
end

function drawwall(id)
 return function(x,y)
  spr(id,x,y)
 end
end

function drawui()
 local infos=stage.name
 color(13)
 print(infos,3,121)
 color(7)
 print(infos,2,120)
end

-->8
--update functions
function updateplayer()
 dx=0
 dy=0
 
 if (btnp(⬅️)) dx=-1
 if (btnp(➡️)) dx=1
 if (btnp(⬆️)) dy=-1
 if (btnp(⬇️)) dy=1
 
 if (canmove(player,{x=dx,y=dy},2)) then
  player.x+=dx
  player.y+=dy
 end
end
