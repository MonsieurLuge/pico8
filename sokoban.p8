object=function(id,action)
  objects:find(id,action,objectnotfound)
end

objectnotfound=function(id)
  print("object ["..id.."] not found")
end

objects={
  find=function(self,id,onsuccess,onerror)
    if self[id] then
      onsuccess(self[id])
    else
      onerror(id)
    end
  end,
