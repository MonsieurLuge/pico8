--draw functions

function drawstage()
 local x,y=stage.x,stage.y
 local w,h=stage.w,stage.h
 local xx,yy=x+w,y+h

 --center the screen
 camera(flr((w*8-124)/2),flr((h*8-124)/2))

 --draw the background
 cls(6)
 rectfill(x*8,y*8,xx*8-1,yy*8-1,5)

 --draw the static elements
 for i=1,w do
  for j=1,h do
   local g=stage.ground[i][j]
   if (g) g.draw(i*8-8,j*8-8)
  end
 end

 --draw the other elements
 for elt in all(stage.elements) do
  elt.draw()
 end

 --draw the player
 drawplayer()

 --reset the camera position
 camera()
 if (debug) print(debug,2,2,2)
end

function drawplayer()
 player.draw()
end

function drawsprite(obj,x,y)
 spr(obj.anim() ,x,y)
end

function drawground(stage)
 for i=1,stage.w do
  for j=1,stage.h do
   stage.ground[i][j]:draw(i,j)
  end
 end
end

function drawwall(id)
 return function(x,y)
  spr(id,x,y)
 end
end

function drawui()
 local infos=stage.name
 color(13)
 print(infos,3,121)
 color(7)
 print(infos,2,120)
end

