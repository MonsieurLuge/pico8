drawStage=function()
  local sx=stage.x
  local sy=stage.y
  local sh=stage.height
  local sw=stage.width
  for y=sy,sy+sh do
    for x=sx,sx+sw do
      if mget(x,y)==0 then
        rectfill(
          sx*8,sy*8,
          (sx+sw-1)*8,(sy+sh-1)*8,
          1
        )
      end
    end
  end
  map(sx,sy,0,0,sw,sh)
  -- for obj in all(stage.objects) do
  --   drawObject(obj)
  -- end
end

